<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - API usage examples</title>
    <meta charset="UTF-8">
    <meta name="description" content="Stock Market">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="~/css/site.css" />

    @*<script type="text/javascript" src="~/js/Chart.min.js"></script>*@
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>*@
    <script type="text/javascript" src="~/lib/chartjs-plugin-annotation-master/chartjs-plugin-annotation.min.js"></script>
    <script type="text/javascript" src="~/js/site.js"></script>
</head>
<body>
    <nav>
        <div class="nav-bar">
            <a asp-area="" asp-controller="Home" asp-action="Index">Home </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="AboutUs">About Us </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Symbols">Symbols </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Chart">Chart </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="BuyOrSell">Buy/Sell </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Gainers">Top Gainers </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="SectorList">Sector Performance </a><text> | </text>
            @*<a asp-area="" asp-controller="Home" asp-action="Sector">Sector Specific </a><text> | </text>*@
            <a asp-area="" asp-controller="Home" asp-action="financials">Financials </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Refresh">Refresh </a><text> | </text>
        </div>
        </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>ISDS, Muma College of Business, University of South Florida</p>
        </footer>
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>