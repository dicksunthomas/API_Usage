@model IEnumerable<API_Usage.Models.Sector>

@{
    ViewData["Title"] = "Sector_list";
}

<h2>Sector_list</h2>


<table class="table">
    <thead>
        <tr>
                
                
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.performance)
                </th>
                
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
           
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.performance)
            </td>
           
           
        </tr>
}
         <tr>
      <td>
        <a id="dbopt" asp-controller="Home" asp-action="PopulateSector"><img src="~/img/button_save-data.png" /></a>
      </td>
             </tr>
    </tbody>
</table>
<script>
    alertDbSave(@ViewBag.dbSuccessComp);
</script>
